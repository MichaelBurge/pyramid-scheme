#lang pyramid

(require psl "primitives.pmd")
(require psl "syntax.pmd")

(set-test-result! 1)

(let ([ x (%-unbox 10)]
      [ y (%-unbox 4)])
  (syntax-fold
   %#-and
   (%#-= (%-unbox 14)  (%#-+ x y))
   (%#-= (%-unbox 6)   (%#-- x y))
   (%#-= (%-unbox 40)  (%#-* x y))
   (%#-= (%-unbox 2)   (%#-u/ x y))
   (%#-= (%-unbox 2)   (%#-u% x y))
   (%#-= (%-unbox 0)   (%#-and x y))
   (%#-= (%-unbox 14)  (%#-or x y))
   (%#-= (%-unbox 14)  (%#-xor x y))
   (%#-= (%-unbox 160) (%#-bitshiftl x y))
   (%#-= (%-unbox 0)   (%#-bitshiftr x y))
   (%#-= (%-unbox 0)   (%#-= x y))
   (%#-= (%-unbox 0)   (%#-not x))
   (%#-= x             (%#-negate (%#-negate x)))
   ))
